android_app {
    name: "TelephonyProvider",
    privileged: true,
    srcs: ["src/**/*.java", "proto/**/*.proto"],
    platform_apis: true,
    certificate: "platform",
    libs: ["telephony-common"],
    static_libs: [
    "android-common",
    "telephonyprovider-protos",
    "android-support-v4"
    ],
}

filegroup {
    name: "telephonyprovider-assets-carrierlist",
    srcs: ["assets/carrier_list.*"],
}
